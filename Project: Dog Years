const myAge = 2; 

let earlyYears = 2; 

earlyYears = earlyYears*10.5; 

laterYears = myAge -2; 

laterYears*= 4; 

myAgeInDogYears = earlyYears + laterYears; 

const myName = 'Andrew'
myName.toLowerCase(); 

console.log('My name is ' + myName + '. I am ' + myAgeInDogYears +' years old in dog years.')
